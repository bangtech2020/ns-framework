{
    "name": "bangtech/ns",
    "description": "NS development Framework,all plug-ins are loaded by Phar",
    "type": "project",
    "license": "GPL-3.0-only",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "bangtech",
            "email": "system@bangtech.cn"
        },
        {
            "name": "hepeichun",
            "email": "hepc@bangtech.cn"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "ext-swoole": ">=4.4",
        "php-di/php-di": "^6.2",
        "inhere/console": "^3.1",
        "symfony/finder": "4.*",
        "nikic/fast-route": "2.0.x-dev",
        "nicmart/string-template": "9999999-dev",
        "prince/swoole-think-orm": "9999999-dev"
    },
    "require-dev": {
        "humbug/box": "^3.8",
        "swoole/ide-helper": "^4.5"
    },
    "autoload": {
        "psr-4": {
            "bootstrap\\" : "bootstrap/",
            "commands\\" : "commands/",
            "extend\\" : "extend/",
            "helper\\": "helper/",
            "interfaces\\": "interfaces/",
            "module\\": "module/",
            "route\\": "route/"
        },
        "files": [
            "functions/common.php"
        ]
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
