{
  "main": "bin/ns.php",
  "output": "build/ns.phar",
  "directories": [
    "bin",
    "bootstrap",
    "brand",
    "commands",
    "config",
    "extend",
    "functions",
    "helper",
    "interfaces",
    "module",
    "public",
    "route"
  ],
  "files-bin": [
    ".env.default"
  ],
  "finder": [
    {
      "notName": "/LICENSE|.*\\.md|.*\\.dist|composer\\.json|composer\\.lock/",
      "exclude": [
        "doc",
        "docs",
        "test",
        "test_old",
        "tests",
        "Tests",
        "vendor-bin"
      ],
      "in": "vendor"
    },
    {
      "name": "composer.json",
      "in": "."
    }
  ],
  "compression": "NONE",
  "compactors": [
    "KevinGH\\Box\\Compactor\\Json",
    "KevinGH\\Box\\Compactor\\Php"
  ],
  "git": "git",
  "git-commit": "git_commit",
  "datetime-format":"datetime_format"
}
